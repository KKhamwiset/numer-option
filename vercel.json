{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/Numer/index.html"
        }
    ],
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "Numer/package.json",
            "use": "@vercel/static-build"
        }
    ]
}