{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "git": {
        "deploymentEnabled": {
        "main": true
        }
    },
    "builds": [
      {
        "src": "Numer/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "Numer/dist/$1"
      }
    ]
  }
  